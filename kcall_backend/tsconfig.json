{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "rootDir": "./",
    "typeRoots": [
      "./node_modules/@types",
      "./types"
    ],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "./types/express.d.ts",
    "./**/*.ts",
  ],
  "exclude": [
    "node_modules"
  ]
}